<%- include('blocks/header', {bot, user, path}) %>
<div class="container">
	<div class="jumbotron">
		<div align="left">
			<a class="navbar-brand"><img src="<%= bot.user.avatarURL %>" width="200" height="200" class="d-inline-block align-top rounded-circle"><br /><font color="white" size=20> <%= bot.user.username %></font></a><br /><br />
			<% if(!user) { %>
				<a class="btn btn-primary" href="/login" role="button">Giriş Yap</a>
				<a class="btn btn-primary" href="/commands" role="button">Komutlar</a>
			<% } else { %>
				<a class="btn btn-primary" href="/dashboard" role="button"><i aria-hidden="true"></i>Yönetim Paneli</a>
				<% if(bot.ayarlar.sahip.includes(user.id)) { %>
						<a class="btn btn-primary" href="/admin" role="button"><i aria-hidden="true"></i>Admin Paneli</a>
				<% } %>
				<a class="btn btn-primary" href="/commands" role="button">Komutlar</a>
				<a class="btn btn-primary" href="/logout" role="button"><i aria-hidden="true"></i>Çıkış Yap</a>
			<% } %>
		</div>
	</div>
</div>
<% include ./blocks/footer %>